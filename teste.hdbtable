COLUMN TABLE "ADDRESSES"(
	"ADDRESSID" NVARCHAR(10) NOT NULL,
	"CITY" NVARCHAR(40),
	"POSTALCODE" NVARCHAR(10),
	"STREET" NVARCHAR(60),
	"BUILDING" NVARCHAR(10),
	"COUNTRY" NVARCHAR(3),
	"REGION" NVARCHAR(4),
	"ADDRESSTYPE" NVARCHAR(2),
	"VALIDITYSTART" DATE,
	"VALIDITYEND" DATE,
	"LAT" REAL,
	"LONG" REAL,
    "ADD" NVARCHAR(4)
	PRIMARY KEY (
		"ADDRESSID"
	)
)
UNLOAD PRIORITY 5 AUTO MERGE;